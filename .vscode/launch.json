    "version": "0.2.0",
    "configurations": [
        {
            "name": "Run Grid reward machine",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "1000",
                "grid-reward-rm",
                "--height",
                "6",
                "--width",
                "6",
                "--grids",
                "5",
                "--horizon",
                "50"
            ]
        },
        {
            "name": "Run Grid exploration",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "100",
                "grid",
                "--height",
                "20",
                "--width",
                "20",
                "--grids",
                "3"
            ]
        },
        {
            "name": "Run paxos part",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "10",
                "paxos-part",
                "--horizon",
                "100",
                "--requests", 
                "3", 
            ]
        },
        {
            "name": "Run paxos reward rm",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "10",
                "--horizon",
                "100",
                "paxos-reward-rm"
            ]
        },
        {
            "name": "Run etcd-raft-bugs",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "100",
                "--horizon",
                "100",
                "etcd-raft-bugs"
            ]
        },
        {
            "name": "Run rsl exploration",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "10",
                "--horizon",
                "100",
                "-r",
                "3",
                "rsl"
            ]
        },
        {
            "name": "Run rsl reward machine",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "1000",
                "--horizon",
                "100",
                "-r",
                "3",
                "rsl-rm",
                "OutOfSync",
            ]
        },
        {
            "name": "Run raft exploration",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "10",
                "--horizon",
                "100",
                "-r",
                "3",
                "raft-part",
                "--runs",
                "1"
            ]
        },
        {
            "name": "Run redis test cli",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "redis-cli",
            ]
        },
        {
            "name": "Run redis raft exploration",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "10",
                "redisraft",
            ]
        },
        {
            "name": "Run redis raft reward machine",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}",
            "args": [
                "-e",
                "10",
                "redisraft-rm",
                "OutOfSync",
            ]
        },
        {
            "name": "Graph analysis",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/scripts/visualize/analyze_graph.py",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "results/visit_graph_RL.json"
            ]
        },
        {
            "name": "Viz server",
            "type": "python",
            "request": "launch",
            "module": "flask",
            "cwd": "${workspaceFolder}/scripts/visualize",
            "env": {
                "FLASK_APP": "viz.py",
                "FLASK_DEBUG": "1",
                "GRAPH_PATH": "${workspaceFolder}/results"
            },
            "args": [
                "run",
                "--no-debugger",
                "--no-reload"
            ],
            "jinja": true,
            "justMyCode": true
        }
    ]
}